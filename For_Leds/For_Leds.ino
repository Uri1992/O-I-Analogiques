//******  Includes  ******************************************************


//******  Variables  *****************************************************

const int led = 11;
int temps = 10;

//*******  Setup  ********************************************************
void setup()
{
  pinMode(led, OUTPUT);
}

//******  Loop  **********************************************************
void loop ()
{
  for (int i = 0; i < 255; i++)
    {
      analogWrite(led, i);
      delay(temps);
    }
    
  for (int i = 255; i > 0; i--)
    {
      analogWrite(led, i);
      delay(temps);
    }
}
  
  
